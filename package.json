    "prepare:quickswap:polygon": "mustache config/quickswap/polygon.json templates/algebra.yaml > subgraph.yaml",
    "prepare:zyberswap:arbitrum": "mustache config/zyberswap/arbitrum.json templates/algebra.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
