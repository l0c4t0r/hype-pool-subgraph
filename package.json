    "deploy:uniswap:polygon": "graph deploy --node https://api.thegraph.com/deploy/ l0c4t0r/hype-pool-uniswap-polygon",
    "deploy:quickswap:polygon": "graph deploy --node https://api.thegraph.com/deploy/ l0c4t0r/hype-pool-quickswap-polygon",
    "deploy:zyberswap:arbitrum": "graph deploy --node https://api.thegraph.com/deploy/ l0c4t0r/hype-pool-zyberswap-arbitrum",
    "deploy:lab": "graph deploy --node https://api.thegraph.com/deploy/ l0c4t0r/laboratory",
